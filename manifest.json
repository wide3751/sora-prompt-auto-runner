{
  "manifest_version": 3,
  "name": "Sora Prompt Auto Runner",
  "version": "1.0.0",
  "description": "自動批量提交 Sora 視頻生成提示詞的 Chrome 擴展工具",
  "author": "Sora Prompt Auto Runner Contributors",
  "homepage_url": "https://github.com/your-username/sora-prompt-auto-runner",
  "action": { "default_popup": "popup.html" },
  "permissions": ["activeTab", "scripting", "storage"],
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  }
}
